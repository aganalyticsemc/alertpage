<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
             xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html"
             xmlns:b.gwt="urn:import:org.gwtbootstrap3.client.ui.gwt"
	         xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="styleTmpPage.css" />
	
	<b:Container fluid="true" addStyleNames="{style.containerPageOne}">

<!-- PAGE TITLE -->
		<b:Row addStyleNames="{style.column}">
			<b:Column size = "SM_12">
				<b:Heading size="H1" alignment="LEFT" text="Alert Page"/>
			</b:Column> 
		</b:Row>	
	<!-- 				UPPER PART -->
	
		<b:Row addStyleNames="{style.column}">

		
<!-- 			1st square -->
				<b:Column size="SM_2"  >
					<b:Jumbotron addStyleNames="{style.upperSquares}">
						<b:Row>
							<b:Heading size="H1" alignment="CENTER" text="22" color="white"/>
						</b:Row>
						<b:Row>
							<b:Heading size="H5" alignment="CENTER" text="New alerts the past 7 days" addStyleNames="{style.upperSquaresDescriptions}"/>
						</b:Row>						
					</b:Jumbotron>
				</b:Column>
				
<!-- 			2nd square -->				
				<b:Column size="SM_2">
					<b:Jumbotron addStyleNames="{style.upperSquares}">
						<b:Row>
							<b:Heading alignment="CENTER" size="H1" text="15" color="white"/>
						</b:Row>
						<b:Row>
							<b:Heading alignment="CENTER" size="H5" text="Open Alerts" addStyleNames="{style.upperSquaresDescriptions}"/>
						</b:Row>						
					</b:Jumbotron>
				</b:Column>
				
<!-- 			3rd square -->					
				<b:Column size="SM_2">
					<b:Jumbotron addStyleNames="{style.upperSquares}">
						<b:Row>
							<b:Heading alignment="CENTER" size="H1" text="3" color="white"/>
						</b:Row>
						<b:Row>
							<b:Heading alignment="CENTER" size="H5" text="Urgent Open Alerts" addStyleNames="{style.upperSquaresDescriptions}"/>
						</b:Row>						
					</b:Jumbotron>
				</b:Column>
				
<!-- 			4th square -->						
				<b:Column size="SM_2">
					<b:Jumbotron addStyleNames="{style.upperSquares}">
						<b:Row>
							<b:Heading alignment="CENTER" size="H1" text="32" color="white"/>
						</b:Row>
						<b:Row>
							<b:Heading alignment="CENTER" size="H5" text="Alerts Handled past 7 days" addStyleNames="{style.upperSquaresDescriptions}"/>
						</b:Row>						
					</b:Jumbotron>
				</b:Column>
				
<!-- 			5th square -->						
				<b:Column size="SM_2">
					<b:Jumbotron addStyleNames="{style.upperSquares}">
						<b:Row>
							<b:Heading alignment="CENTER" size="H1" text="15" color="white"/>
						</b:Row>
						<b:Row>
							<b:Heading alignment="CENTER" size="H5" text="Customers retained past 30 days" addStyleNames="{style.upperSquaresDescriptions}"/>
						</b:Row>						
					</b:Jumbotron>
				</b:Column>
				
<!-- 			6th square -->						
				<b:Column size="SM_2">
					<b:Jumbotron addStyleNames="{style.upperSquares}">
						<b:Row>
							<b:Heading alignment="CENTER" size="H1" text="69" color="white"/>
						</b:Row>
						<b:Row>
							<b:Heading alignment="CENTER" size="H5" text="Customers retained past 90 days" addStyleNames="{style.upperSquaresDescriptions}"/>
						</b:Row>						
					</b:Jumbotron>
				</b:Column>					
																							
		
			</b:Row>	
				
				
<!-- 				LOWER PANEL -->
				<b:Row addStyleNames="{style.column}">	
				<b:Column size = "SM_12" >
				<b:Panel>
<!-- 					<b:PanelHeader addStyleNames="{style.dataTableHeader}"> -->
<!-- 						<b:Heading size="H3" text="Table of alerts"/> -->
<!-- 					</b:PanelHeader> -->
					<b:PanelBody>
						<b:Heading alignment="LEFT" size="H3" text="Table of Alerts"/>
						<b:Row/>
						<b:NavTabs addStyleNames="{style.dataTableNavTabs}">
							<b:ListDropDown>
								<b:AnchorButton active="true" dataToggle="DROPDOWN" text="Attention Level"/>
									<b:DropDownMenu>
										<b:AnchorListItem>Urgent</b:AnchorListItem>
										<b:AnchorListItem>High</b:AnchorListItem>
										<b:AnchorListItem>Low</b:AnchorListItem>
									</b:DropDownMenu>								
							</b:ListDropDown>
							<b:ListDropDown>
								<b:AnchorButton dataToggle="DROPDOWN" text="Segment"/>
									<b:DropDownMenu>
										<b:AnchorListItem>Primary</b:AnchorListItem>
										<b:AnchorListItem>Secondary</b:AnchorListItem>
										<b:AnchorListItem>Tertiary</b:AnchorListItem>
									</b:DropDownMenu>								
							</b:ListDropDown>
							<b:AnchorListItem icon="FILE_EXCEL_O" pull="RIGHT"></b:AnchorListItem>
							<b:AnchorListItem icon="PRINT" pull="RIGHT"></b:AnchorListItem>
							<b:AnchorListItem icon="FILE_PDF_O" pull="RIGHT"></b:AnchorListItem>
						    <b:NavbarForm pull="RIGHT">
						      <b:TextBox placeholder="Search Company Name..."/>
						    </b:NavbarForm>
						    						    			
						</b:NavTabs>					
                    <b.gwt:DataGrid bordered="false" condensed="true" striped="false" hover="true" height="500px"
                                    width="100%" ui:field="dataGrid" addStyleNames="{style.dataTableContent}" />
                    <b:Pagination ui:field="dataGridPagination"/>					
					</b:PanelBody>
				</b:Panel>
						
			</b:Column>
		</b:Row>
	</b:Container>				


</ui:UiBinder> 